{
  "manifest_version": 3,
  "name": "Youtube Dark Mode",
  "version": "0.1.2",
  "description": "Extension for Change Youtube to dark mode! easy!!",
  "content_scripts": [{
    "css": ["youtubeOveride.css"],
    "js": ["content.js"],
    "matches": ["https://www.youtube.com/*"]
  }],
  "icons": {
  		"16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png" },
   	"browser_action": {
    	"default_icon": "icon48.png"
	}

}
